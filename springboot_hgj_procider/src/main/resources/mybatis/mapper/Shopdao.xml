<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jk.dao.ShopDao" >



    <insert id="addShop" parameterType="com.jk.model.Shop">
    insert into t_shop(shopName,shopPhone,shopIntro,shopUrl,shopThearea)
    values(#{shopName},#{shopPhone},#{shopIntro},#{shopUrl},#{shopThearea})
  </insert>



    <insert id="addlongShop" parameterType="com.jk.model.longShop">
    insert into t_longShop(name,credit,phone,deposit,bank,url,bankname,ic)
    values(#{name},#{credit},#{phone},#{deposit},#{bank},#{url},#{bankname},#{ic})
  </insert>

</mapper>